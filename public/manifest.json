{
  "name": "Schemator",
  "description": "Create database schema and export to Laravel assets",
  "version": "1.0.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["scripts/background.js"]
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["contextMenus", { "fileSystem": ["write"] }],
  "minimum_chrome_version": "35"
}
